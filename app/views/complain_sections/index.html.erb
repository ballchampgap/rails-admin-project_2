<p id="notice"><%= notice %></p>

<h1>Complain Sections</h1>

<table>
  <thead>
    <tr>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @complain_sections.each do |complain_section| %>
      <tr>
        <td><%= complain_section.section %></td>
        <td><%= link_to 'Show', complain_section %></td>
        <td><%= link_to 'Edit', edit_complain_section_path(complain_section) %></td>
        <td><%= link_to 'Destroy', complain_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Complain Section', new_complain_section_path %>
